{
  "manifest_version": 3,
  "name": "Notate",
  "description": "",
  "version": "1.0",
  "permissions": ["storage", "tabs", "activeTab", "history" ],
  "action": {
	  "default_title": "Open Notate"
  },
 "background": {
	"service_worker": "background.pack.js",
	"type": "module"
  },
  "content_scripts": [{
	"matches": ["https://*/*", "http://*/*"],
	"js": ["content.pack.js"],
	"run_at": "document_end"
  }],
  "web_accessible_resources": [
	{
		"resources":[],
		"matches": []
	}
  ]
}
